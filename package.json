{
    "name": "tester-assistant",
    "version": "1.0.5",
    "description": "helper for tester",
    "scripts": {
        "postinstall": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "test": "jest --no-cache",
        "build": "yarn clean && yarn copy && concurrently \"npm:build-*\" ",
        "build-chrome": "yarn clean:chrome && yarn copy:chrome && webpack --mode=production --config-name chrome",
        "build-firefox": "yarn clean:firefox && yarn copy:firefox && webpack --mode=production --config-name firefox",
        "lint": "eslint src/**/*.{js,ts,tsx}",
        "lint:fix": "npm run lint -- --fix",
        "clean": "concurrently \"npm:clean:*\"",
        "clean:chrome": "cleandir chrome-extension/",
        "clean:firefox": "cleandir firefox-extension/",
        "copy": "concurrently \"npm:copy:*\"",
        "copy:chrome": "concurrently \"npm:copy:chrome:*\"",
        "copy:chrome:html": "cpy src/commonInExtensions/*.html chrome-extension/",
        "copy:chrome:icons": "cpy icons/ chrome-extension/icons",
        "copy:chrome:manifest": "cpy src/manifests/manifest_v3.json chrome-extension --rename=manifest.json",
        "copy:firefox": "concurrently \"npm:copy:firefox:*\"",
        "copy:firefox:html": "cpy src/commonInExtensions/*.html firefox-extension/",
        "copy:firefox:icons": "cpy icons/ firefox-extension/icons/",
        "copy:firefox:manifest": "cpy src/manifests/manifest_v2.json firefox-extension --rename=manifest.json",
        "dev": "yarn copy && webpack --watch --mode=development --config-name chrome"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn test"
        }
    },
    "author": "kulikovaev <kulikovaev@krista.ru>",
    "license": "MIT",
